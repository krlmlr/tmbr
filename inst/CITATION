citHeader("To cite tmbr in publications use:")

citEntry(entry = "misc",
  author       = as.person("Joe Thorley"),
  year         = "2018",
  title        = "tmbr: Analyses Using TMB",
  doi          = "https://doi.org/10.5281/zenodo.1162374",
  textVersion  = paste(
  "Joe Thorley (2018)",
  "tmbr: Analyses Using TMB.",
  "doi: https://doi.org/10.5281/zenodo.1162374."))

citFooter("Please also cite TMB.")
